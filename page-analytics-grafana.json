      "targets": [
        {
          "dataset": "page_analytics",
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH events AS (\r\n    SELECT hour, page_load_time_ms\r\n    FROM fact_events\r\n    JOIN dim_time\r\n    ON fact_events.time_key = dim_time.time_key\r\n    WHERE $__timeFilter(timestamp AT TIME ZONE 'IST') \r\n)\r\nSELECT \r\n    COALESCE(AVG(page_load_time_ms), 0) as page_load_time,\r\n    CONCAT(h.hour_of_day, '-', h.hour_of_day+1) as hour_of_day\r\nFROM \r\n    GENERATE_SERIES(0, 23) AS h(hour_of_day)\r\n    LEFT JOIN events\r\n    ON events.hour = h.hour_of_day\r\nGROUP BY\r\n    h.hour_of_day\r\nORDER BY\r\n    h.hour_of_day",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [
                  {
                    "name": "page_load_time_ms",
                    "type": "functionParameter"
                  }
                ],
                "type": "function"
              }
            ],
            "groupBy": [
              {
                "property": {
                  "type": "string"
                },
                "type": "groupBy"
              }
            ],
            "limit": 50
          },
          "table": "fact_events"
        }
      ],
      "title": "Average Page Load time / Hour",
